  keywords = {Computer Science - Information Theory,Computer Science - Machine Learning,Computer Science - Social and Information Networks,Statistics - Machine Learning},
  file = {/home/henrik/Zotero/storage/WJUGNSIP/Bouritsas et al. - 2021 - Partition and Code learning how to compress graph.pdf;/home/henrik/Zotero/storage/9L4SXEJW/2107.html}
