  keywords = {Complexity theory,Degradation,Image recognition,Image segmentation,Neural networks,Training,Visualization},
  file = {/home/henrik/Zotero/storage/XHBQM3PI/He et al. - 2016 - Deep Residual Learning for Image Recognition.pdf;/home/henrik/Zotero/storage/2RQRNJC5/7780459.html}
